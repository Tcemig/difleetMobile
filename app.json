{
  "expo": {
    "name": "difleetMobile",
    "slug": "difleetMobile",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/deliver-it_NewLogoOnly_1024.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "permissions": ["LOCATION"],
    "splash": {
      "image": "./assets/images/deliver-it_NewLogoOnly_1024.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.deliverit.difleet",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to track your position during the inspection."
        }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/deliver-it_NewLogoOnly_108.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.deliverit.difleet",
      "versionCode": 1,
      "permissions": ["ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0e680239-8621-44fb-aa17-b28905094c3a"
      }
    },
    "owner": "tyleremig"
  }
}
